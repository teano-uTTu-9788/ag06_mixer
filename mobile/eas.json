            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_RC_API_KEY": "appl_dev_placeholder_key",
                "EXPO_PUBLIC_API_URL": "http://192.168.1.50:8899"
